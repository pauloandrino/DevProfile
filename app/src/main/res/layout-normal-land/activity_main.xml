<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                             xmlns:app="http://schemas.android.com/apk/res-auto"
                                             xmlns:tools="http://schemas.android.com/tools"
                                             android:layout_width="match_parent"
                                             android:layout_height="match_parent">

    <ImageView
            android:layout_width="0dp"
            android:layout_height="120dp" app:srcCompat="@drawable/devslopesbanner"
            android:id="@+id/imageView17"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintHorizontal_bias="0.0"
            android:background="@color/colorAccent"/>
    <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" app:srcCompat="@drawable/devslopesprofilelogo"
            android:id="@+id/logo"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="36dp"
            android:layout_marginTop="100dp" app:layout_constraintTop_toTopOf="parent"/>
    <TextView
            android:text="Devlopes"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView3" android:textSize="24sp" app:layout_constraintStart_toEndOf="@+id/logo"
            android:layout_marginStart="32dp" android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/camerabutton"
            android:id="@+id/imageView22"
            app:layout_constraintStart_toEndOf="@+id/textView3" app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintEnd_toStartOf="@+id/imageView23"
            app:layout_constraintHorizontal_chainStyle="packed"
            app:layout_constraintTop_toTopOf="@+id/imageView23"
            app:layout_constraintBottom_toBottomOf="@+id/imageView23"/>
    <ImageView
            android:layout_width="50dp"
            android:layout_height="50dp" app:srcCompat="@drawable/favoritebutton"
            android:id="@+id/imageView23"
            app:layout_constraintStart_toEndOf="@+id/imageView22" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toStartOf="@+id/imageView24" app:layout_constraintTop_toTopOf="@+id/imageView24"
            app:layout_constraintBottom_toBottomOf="@+id/imageView24" android:layout_marginStart="32dp"/>
    <ImageView
            android:layout_height="50dp" app:srcCompat="@drawable/inboxbutton"
            android:id="@+id/imageView24"
            android:layout_width="50dp"
            app:layout_constraintStart_toEndOf="@+id/imageView23" app:layout_constraintHorizontal_bias="0.5"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView17" android:layout_marginStart="32dp"
            android:layout_marginEnd="16dp"/>
    <android.support.constraint.Guideline android:layout_width="wrap_content" android:layout_height="wrap_content"
                                          android:id="@+id/guideline3"
                                          android:orientation="vertical"
                                          app:layout_constraintGuide_percent="0.5"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/codeimage"
            android:id="@+id/imageView25"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="@+id/imageView26" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/imageView26" app:layout_constraintDimensionRatio="1:1"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/videosimage"
            android:id="@+id/imageView26"
            android:scaleType="centerCrop" app:layout_constraintStart_toStartOf="@+id/guideline3"
            android:layout_marginStart="8dp"
            app:layout_constraintTop_toTopOf="@+id/imageView28"
            app:layout_constraintBottom_toBottomOf="@+id/imageView28"
            app:layout_constraintEnd_toStartOf="@+id/imageView25"
            android:layout_marginEnd="16dp" app:layout_constraintDimensionRatio="1:1"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/dataimage"
            android:id="@+id/imageView27"
            app:layout_constraintEnd_toStartOf="@+id/imageView28"
            android:layout_marginEnd="16dp"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="16dp" app:layout_constraintDimensionRatio="1:1"
            android:layout_marginTop="16dp" app:layout_constraintTop_toBottomOf="@+id/logo"
            app:layout_constraintHorizontal_bias="0.431"
            android:layout_marginBottom="16dp" app:layout_constraintBottom_toBottomOf="parent"
            android:scaleType="centerCrop"/>
    <ImageView
            android:layout_width="0dp"
            android:layout_height="0dp" app:srcCompat="@drawable/imagesimage"
            android:id="@+id/imageView28"
            app:layout_constraintEnd_toStartOf="@+id/guideline3"
            android:layout_marginEnd="8dp" app:layout_constraintDimensionRatio="1:1"
            app:layout_constraintTop_toTopOf="@+id/imageView27"
            app:layout_constraintBottom_toBottomOf="@+id/imageView27" android:scaleType="centerCrop"/>
</android.support.constraint.ConstraintLayout>